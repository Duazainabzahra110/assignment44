#include <iostream>
#include <vector>
using namespace std;

void reverseVector(vector<int>& v) {
    int i = 0, j = v.size() - 1;
    while (i < j) {
        int temp = v[i];
        v[i] = v[j];
        v[j] = temp;
        i++;
        j--;
    }
}
int main() {
    vector<int> v1 = {10, 20, 30, 40, 50};
    for (int i = 0; i < v1.size(); i++) {
        cout << v1[i] << " ";
    }
    cout << endl;
    for (int x : v1) {
        cout << x << " ";
    }
    cout << endl;
    int N;
    cin >> N;
    vector<int> v;
    for (int i = 0; i < N; i++) {
        int num;
        cin >> num;
        v.push_back(num);
    }
    int max = v[0];
    int min = v[0];
    for (int i = 1; i < v.size(); i++) {
        if (v[i] > max)
            max = v[i];
        if (v[i] < min)
            min = v[i];
    }
    cout << max << endl;
    cout << min << endl;
    reverseVector(v);
    for (int x : v) {
        cout << x << " ";
    }
    cout << endl;
    int target, count = 0;
    cin >> target;
    for (int i = 0; i < v.size(); i++) {
        if (v[i] == target)
            count++;
    }
    cout << count << endl;
    return 0;
}
