#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<int> v;
    for (int i = 0; i < n; i++) {
        int x;
        cin >> x;
        v.push_back(x);
    }
    for (int i = 0; i < v.size(); ) {
        if (v[i] % 2 == 0)
            v.erase(v.begin() + i);
        else
            i++;
    }
    for (int x : v)
        cout << x << " ";
    cout << endl;
    int n1;
    cin >> n1;
    vector<int> a(n1), b(n1 - 1);
    for (int i = 0; i < n1; i++)
        cin >> a[i];
    for (int i = 0; i < n1 - 1; i++)
        cin >> b[i];
    int sum1 = 0, sum2 = 0;
    for (int x : a)
        sum1 += x;
    for (int x : b)
        sum2 += x;
    cout << sum1 - sum2 << endl;
    bool duplicate = false;
    for (int i = 0; i < v.size(); i++) {
        for (int j = i + 1; j < v.size(); j++) {
            if (v[i] == v[j])
                duplicate = true;
        }
    }
    if (duplicate)
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
    int target;
    cin >> target;
    int count = 0;
    for (int i = 0; i < v.size(); i++) {
        for (int j = i + 1; j < v.size(); j++) {
            if (v[i] + v[j] == target)
                count++;
        }
    }
    cout << count << endl;
    return 0;
}
